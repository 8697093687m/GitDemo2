package ui2;

import org.testng.annotations.AfterMethod;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class Groupingpractice {
	
	
	@Test(priority=1,description="For t1 runs check",groups="Regression")
	public void test1(){
		System.out.println("t1 runs successfully");
	}
	@Test(priority=2,description="For t2 runs check",groups="Regression")
	public void t2() {
		System.out.println("t2 runs successfully");
	}
	@Test(priority=3,description="For t3 runs check",groups= {"Regression","Sanity"})
	public void t3() {
		System.out.println("t3 runs successfully");
	}
	@Test(priority=4,description="For t2 runs check",groups="Sanity")
	public void t4() {
		System.out.println("t4 runs successfully");
	}

}

